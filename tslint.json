{
  "extends": ["tslint:recommended", "tslint-eslint-rules", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": true,
    "adjacent-overload-signatures": true,
    "ban-types": [
      true,
      ["Object", "Avoid using 'Object' type. Did you mean 'object'?"],
      ["String", "Avoid using 'String' type. Did you mean 'string'?"],
      ["Number", "Avoid using 'Number' type. Did you mean 'number'?"],
      ["Boolean", "Avoid using 'Boolean' type. Did you mean 'boolean'?"]
    ],
    "member-access": [true, "check-accessor", "check-parameter-property"],
    "member-ordering": [
      true,
      {
        "order": [
          "private-static-field",
          "protected-static-field",
          "public-static-field",
          "private-instance-field",
          "protected-instance-field",
          "public-instance-field",
          "private-constructor",
          "protected-constructor",
          "public-constructor",
          "private-static-method",
          "protected-static-method",
          "public-static-method",
          "private-instance-method",
          "protected-instance-method",
          "public-instance-method"
        ],
        "alphabetize": false
      }
    ],
    "no-any": false,
    "no-empty-interface": false,
    "no-import-side-effect": [
      true,
      { "ignore-module": "(\\.html|\\.css|\\.less|\\.sass|\\.scss)$" }
    ],
    "no-inferrable-types": true,
    "no-internal-module": true,
    "no-magic-numbers": false,
    "no-namespace": [true, "allow-declarations"],
    "no-non-null-assertion": false,
    "no-parameter-reassignment": false,
    "no-reference": true,
    "no-unnecessary-type-assertion": true,
    "no-var-requires": true,
    "only-arrow-functions": [false],
    "prefer-for-of": true,
    "promise-function-async": false,
    "typedef": true,
    "unified-signatures": false,
    "await-promise": false,
    "ban-comma-operator": true,
    "ban": false,
    "curly": true,
    "forin": true,
    "import-blacklist": false,
    "label-position": false,
    "no-arg": true,
    "no-bitwise": false,
    "no-conditional-assignment": true,
    "no-console": true,
    "no-construct": true,
    "no-debugger": true,
    "no-duplicate-super": true,
    "no-duplicate-switch-case": true,
    "no-duplicate-variable": [true, "check-parameters"],
    "no-dynamic-delete": false,
    "no-eval": true,
    "no-empty": true,
    "no-floating-promises": false,
    "no-for-in-array": false,
    "no-implicit-dependencies": [true, "dev"],
    "no-inferred-empty-object-type": false,
    "no-invalid-template-strings": true,
    "no-invalid-this": false,
    "no-misused-new": true,
    "no-null-keyword": false,
    "no-object-literal-type-assertion": false,
    "no-return-await": true,
    "no-shadowed-variable": false,
    "no-sparse-arrays": true,
    "no-string-literal": false,
    "no-string-throw": true,
    "no-submodule-imports": false,
    "no-switch-case-fall-through": true,
    "no-unsafe-any": false,
    "no-unsafe-finally": true,
    "no-unused-expression": true,
    "no-unused-variable": true,
    "no-use-before-declare": false,
    "no-var-keyword": true,
    "no-void-expression": true,
    "prefer-conditional-expression": true,
    "prefer-object-spread": true,
    "radix": false,
    "restrict-plus-operands": true,
    "strict-boolean-expressions": false,
    "strict-type-predicates": true,
    "switch-default": true,
    "triple-equals": true,
    "typeof-compare": false,
    "use-default-type-parameter": true,
    "use-isnan": true,
    "cyclomatic-complexity": [true, 20],
    "deprecation": false,
    "max-classes-per-file": [true, 1],
    "max-file-line-count": [true, 1000],
    "no-default-export": false,
    "no-duplicate-imports": true,
    "no-mergeable-namespace": true,
    "no-require-imports": false,
    "object-literal-sort-keys": false,
    "prefer-const": true,
    "prefer-readonly": true,
    "array-type": false,
    "arrow-return-shorthand": true,
    "binary-expression-operand-order": true,
    "callable-types": true,
    "class-name": true,
    "comment-format": [true, "check-space"],
    "completed-docs": false,
    "encoding": true,
    "file-header": false,
    "file-name-casing": [true, "kebab-case"],
    "interface-name": true,
    "interface-over-type-literal": true,
    "jsdoc-format": [true, "check-multiline-start"],
    "match-default-export-name": false,
    "no-angle-bracket-type-assertion": true,
    "no-boolean-literal-compare": true,
    "no-parameter-properties": false,
    "no-redundant-jsdoc": true,
    "no-reference-import": true,
    "no-unnecessary-callback-wrapper": false,
    "no-unnecessary-initializer": true,
    "no-unnecessary-qualifier": false,
    "object-literal-shorthand": true,
    "one-variable-per-declaration": [true, "ignore-for-loop"],
    "ordered-imports": true,
    "prefer-function-over-method": false,
    "prefer-method-signature": true,
    "prefer-switch": false,
    "prefer-template": true,
    "prefer-while": true,
    "return-undefined": true,
    "switch-final-break": false,
    "type-literal-delimiter": true,
    "variable-name": [true, "ban-keywords"]
  }
}
